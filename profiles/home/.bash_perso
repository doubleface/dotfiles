. /etc/bash_completion
shopt -s globstar

$(find $HOME/Workspace -maxdepth 2 -type d -name .sub | xargs -n 1 -I{} find {}/bin/ -type l | xargs -I{} echo '{} init -')>/tmp/sublist
source /tmp/sublist
rm -f /tmp/sublist

source /usr/share/autojump/autojump.bash

if [ -z "$SSH_AUTH_SOCK" ] ; then
    eval `ssh-agent -s`
    ssh-add
fi
